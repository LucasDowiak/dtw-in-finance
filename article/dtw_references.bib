@article{Gatev_et_al_2006,
  author = {Gatev, Evan and Goetzmann, William N. and Rouwenhorst, K. Geert},
  title = {Pairs Trading: Performance of a Relative-Value Arbitrage Rule},
  journal = {Review of Financial Studies},
  volume = {19},
  number = {3},
  pages = {797-827},
  year = {2006},
  month = {02},
  abstract = {We test a Wall Street investment strategy, pairs trading, with daily data from 1962 through 2002. Stocks are matched into pairs with minimum distance between normalized historical prices. A simple trading rule yields average annualized excess returns of up to 11 percent for self-financing portfolios of pairs. The profits typically exceed conservative transaction-cost estimates. Bootstrap results suggest that the “pairs” effect differs from previously documented reversal profits. Robustness of the excess returns indicates that pairs trading profits from temporary mispricing of close substitutes. We link the profitability to the presence of a common factor in the returns, different from conventional risk measures.},
  issn = {0893-9454},
  doi = {10.1093/rfs/hhj020},
  url = {https://doi.org/10.1093/rfs/hhj020},
