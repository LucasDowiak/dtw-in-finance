	Journal = {Knowledge and Information Systems},
	Number = {3},
	Pages = {358--386},
	Title = {Exact indexing of dynamic time warping},
	Ty = {JOUR},
	Url = {https://doi.org/10.1007/s10115-004-0154-9},
	Volume = {7},
	Year = {2005},
